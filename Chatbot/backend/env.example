# Environment Configuration for DecentraBot with uAgent Integration

# Server Configuration
PORT=5000
NODE_ENV=development

# uAgent Configuration
UAGENT_SEED=decentrabot_seed_phrase_for_autonomous_web3_operations
UAGENT_PORT=8000

# Agentverse Configuration
AGENTVERSE_API_KEY=your_agentverse_api_key_here
AGENTVERSE_API_BASE=https://api.agentverse.ai/v1
AGENTVERSE_AGENT_ID=decentrabot_agent_id

# Fetch.ai Network Configuration
FETCH_NETWORK_RPC=https://rest-fetchhub.fetch.ai
FETCH_NETWORK_CHAIN_ID=fetchhub-4
FETCH_WALLET_ADDRESS=your_fetch_wallet_address

# ASI One Configuration (when available)
ASI_API_KEY=your_asi_api_key_here
ASI_API_BASE=https://api.asi.one/v1

# Security Configuration
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=logs/decentrabot.log
